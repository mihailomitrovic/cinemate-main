{
  "expo": {
    "name": "CineMate",
    "slug": "cinemate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../assets/icon1.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "../assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "packagerOpts": {
      "sourceExts": ["cjs"]
    }
  }
}
